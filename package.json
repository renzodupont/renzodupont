{
  "name": "renzodupont-tech",
  "version": "1.0.0",
  "description": "Blog de tecnología y análisis técnico - Renzo Dupont",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "dev": "node app.js",
    "generate-images": "node generate-images-simple.js && node update-post-images.js",
    "generate-images-smart": "node generate-images-smart.js && node update-post-images.js",
    "generate-images-all": "node generate-images-smart.js --all && node update-post-images.js",
    "organize-posts": "node organize-posts.js",
    "organize-posts-dry": "node organize-posts.js --dry-run",
    "update-links": "node update-post-links.js",
    "update-links-dry": "node update-post-links.js --dry-run",
    "update-images": "node update-post-images.js",
    "update-images-dry": "node update-post-images.js --dry-run",
    "workflow": "./complete-workflow.sh",
    "workflow-dry": "./complete-workflow.sh --dry-run",
    "create-post": "node orchestrate-post.js",
    "create-post-featured": "node orchestrate-post.js --featured",
    "create-post-no-image": "node orchestrate-post.js --no-image",
    "create-content": "node generate-content.js",
    "regenerate-post": "node regenerate-post.js",
    "regenerate-all": "node regenerate-post.js --all",
    "regenerate-titles": "node regenerate-titles.js",
    "regenerate-titles-dry": "node regenerate-titles.js --dry-run"
  },
  "keywords": [
    "tecnología",
    "programación",
    "desarrollo web",
    "uruguay",
    "español",
    "tech blog"
  ],
  "author": "Renzo Dupont",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.22.0",
    "@google/generative-ai": "^0.21.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "jsdom": "^27.0.0",
    "node-fetch": "^2.7.0"
  }
}
